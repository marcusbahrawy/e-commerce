{
    "name": "motorleaks/e-commerce",
    "description": "Høyytelses nettbutikk — LAMP, PHP 8+, uten WordPress",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=8.2",
        "stripe/stripe-php": "^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Support/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
